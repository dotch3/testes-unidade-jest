{
  "name": "TestesUnidadeJest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest ./src"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/runtime": "^7.11.1",
    "babel-jest": "^26.2.2",
    "jest": "^26.2.2",
    "metro-react-native-babel-preset": "^0.61.0",
    "react-dom": "^16.13.1",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "testMatch": [
      "**/?(*.)(spec).(js|jsx)?(x)"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    }
  },
  "dependencies": {
    "winston": "^3.3.3"
  }
}
